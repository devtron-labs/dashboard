{
    "name": "devtron-frontend",
    "version": "1.0.0",
    "scripts": {
        "prepare": "husky install",
        "lint": "pnpm run -r lint",
        "start": "pnpm run --filter web start",
        "build:prod": "pnpm run -r build",
        "build:libs": "pnpm --filter './packages/**' build",
        "build:storybook": "pnpm --filter web build-storybook",
        "start:storybook": "pnpm --filter web storybook"
    },
	"devDependencies": {
		"husky": "^7.0.4"
	}
}
